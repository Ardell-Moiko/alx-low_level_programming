#include "main.h"
#include <stdlib.h>
#include <stdio.h>
/**
 * main - prints the sum of integers.
 * @argc: number of commands entered in argv
 * @argv: value of commands entered
 * Return: Always 0
 */

int main(int argc, char *argv[])
{
int i, sum = 0;

if (argc <= 1)
{
printf("0\n");
}
if (*argv >= ('a' || 'A') && *argv <= ('z' || 'Z'))
{
printf("Error\n");
return 1;
}
for (i = 1; i < argc; i++)
{
sum += atoi(argv[i]);
}
printf("%d\n", sum);
return (0);
}
